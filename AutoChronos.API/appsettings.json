{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=your_host;Database=AutoChronosDB;Username=${DB_USERNAME};Password=${DB_PASSWORD}"
  },
  "SendGridApiKey": "{DB_API_KEY}",
  "EmailSettings": {
    "SenderAddress": "${EMAIL_SENDER_ADDRESS}"
  }
}
